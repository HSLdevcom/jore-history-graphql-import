{
  "name": "jore-history-postgres-import",
  "version": "0.0.1",
  "description": "Data importer for jore-history-graphql",
  "scripts": {
    "start": "node -r esm -r dotenv/config src/index.js",
    "start:production": "forever start -c \"node -r esm -r dotenv/config\" src/index.js && forever -f logs 0",
    "lint": "eslint src --fix",
    "prettier": "prettier */**.{js,json} --write",
    "ftpsource": "ftp-srv ftp://127.0.0.1:2121 --root ./source",
    "knex": "knex",
    "knex:local": "PG_CONNECTION_STRING=postgres://postgres:mysecretpassword@localhost:5678/postgres knex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/jore-history-graphql-import.git"
  },
  "keywords": [],
  "author": "Daniel Dunderfelt <daniel@developsuperpowers.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HSLdevcom/jore-history-graphql-import/issues"
  },
  "homepage": "https://github.com/HSLdevcom/jore-history-graphql-import",
  "dependencies": {
    "basic-ftp": "^3.5.0",
    "chardet": "^0.7.0",
    "cron": "^1.7.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "etl": "^0.6.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-fileupload": "^1.1.5",
    "express-react-views": "^0.11.0",
    "forever": "^1.0.0",
    "fs-extra": "^7.0.0",
    "ftp-srv": "^4.1.0",
    "iconv-lite": "^0.4.24",
    "knex": "^0.16.2",
    "knex-postgis": "^0.7.0",
    "lodash": "^4.17.10",
    "lru-cache": "^5.1.1",
    "p-map": "^2.1.0",
    "p-queue": "^3.0.0",
    "pg": "^7.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "split2": "^3.1.1",
    "string-to-stream": "^1.1.1",
    "through2": "^3.0.1",
    "through2-concurrent": "^2.0.0",
    "unique-stream": "^2.3.1",
    "unzipper": "^0.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier": "^1.14.2"
  }
}
