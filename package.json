{
  "name": "jore-history-postgres-import",
  "version": "0.0.1",
  "description": "Data importer for jore-history-graphql",
  "scripts": {
    "start": "node --max_old_space_size=2048 -r esm -r dotenv/config src/index.js",
    "lint": "eslint src --fix",
    "prettier": "prettier */**.{js,json} --write",
    "ftpsource": "ftp-srv ftp://127.0.0.1:2121 --root ./source",
    "knex": "knex",
    "knex:local": "PG_CONNECTION_STRING=postgres://postgres:mysecretpassword@localhost:5678/postgres knex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/jore-history-graphql-import.git"
  },
  "keywords": [],
  "author": "Daniel Dunderfelt <daniel@developsuperpowers.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HSLdevcom/jore-history-graphql-import/issues"
  },
  "homepage": "https://github.com/HSLdevcom/jore-history-graphql-import",
  "dependencies": {
    "basic-ftp": "^3.5.0",
    "chardet": "^0.7.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "fs-extra": "^7.0.0",
    "ftp-srv": "^4.1.0",
    "iconv-lite": "^0.4.24",
    "knex": "^0.16.2",
    "knex-postgis": "^0.7.0",
    "lodash": "^4.17.10",
    "p-each-series": "^1.0.0",
    "p-map": "^2.1.0",
    "p-queue": "^3.0.0",
    "peek-stream": "^1.1.3",
    "pg": "^7.7.1",
    "stream-splicer": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier": "^1.14.2"
  }
}
